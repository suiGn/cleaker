<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>OrgBoat</title>
    <link rel="stylesheet" href="./sl/fontawesome/css/all.min.css" />
    <link rel=”stylesheet”
    href=”https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css&#8221;
    />
    <!-- Favicon -->
    <link rel="icon" href="./sl/dist/media/img/favicon.png" type="image/png" />
    <!-- Bundle Styles -->
    <link rel="stylesheet" href="./sl/vendor/bundle.css" />
    <link rel="stylesheet" href="./sl/vendor/enjoyhint/enjoyhint.css" />
    <!-- App styles -->
    <link rel="stylesheet" href="./sl/dist/css/app.css" />
  </head>

  <body class="<%= modo %>">
    <!-- page loading -->
    <div class="page-loading"></div>
    <!-- ./ page loading -->
    <!-- page tour modal 
<% include ../partials/workspace/modals/pageTour.ejs %>
 ./ page tour modal -->
    <!-- disconnected modal -->
    <% include ../partials/workspace/modals/disconnected.ejs %>
    <!-- ./ disconnected modal -->
    <!-- voice call modal -->
    <% include ../partials/workspace/modals/voiceCall.ejs %>
    <!-- ./ voice call modal -->
    <!-- video call modal -->
    <% include ../partials/workspace/modals/videoCall.ejs %>
    <!-- ./ video call modal -->
    <!-- add friends modal -->
    <% include ../partials/workspace/modals/addContact.ejs %>
    <!-- ./ add friends modal -->
    <!-- new group modal -->
    <% include ../partials/workspace/modals/newGroup.ejs %>
    <!-- ./ new group modal -->
    <!-- setting modal -->
    <% include ../partials/workspace/modals/settings.ejs %>
    <!-- ./ setting modal -->
    <!-- edit profile modal -->
    <% include ../partials/workspace/modals/editProfile.ejs %>
    <!-- ./ edit profile modal -->
    <!-- edit profile Pic -->
    <% include ../partials/workspace/modals/changeProfilePic.ejs %>
    <!-- ./ edit profile modal -->
    <!-- layout -->
    <div class="layout">
      <!-- navigation -->
      <% include ../partials/workspace/navigation.ejs %>
      <!-- ./ navigation -->
      <!-- content -->
      <div class="content">
        <!-- sidebar group -->
        <div class="sidebar-group">
          <!-- Chats sidebar -->
          <% include ../partials/workspace/sidebars/chats.ejs %>
          <!-- ./ Chats sidebar -->
          <!-- Friends sidebar -->
          <% include ../partials/workspace/sidebars/contacts.ejs %>
          <!-- ./ Friends sidebar -->
          <!-- Favorites (starred) sidebar -->
          <% include ../partials/workspace/sidebars/favorites.ejs %>
          <!-- ./ Favorites (starred) sidebar -->
          <!-- Archived sidebar -->
          <% include ../partials/workspace/sidebars/archived.ejs %>
          <!-- ./ Archived sidebar -->
        </div>
        <!-- ./ sidebar group -->
        <!-- chat -->
        <% include ../partials/workspace/chat.ejs %>
        <!-- ./ chat -->
        <!-- Right Column Sidebar-->
        <% include ../partials/workspace/sidebars/right_sidebar.ejs %>
        <!--./ Right Column Sidebar-->
        <!-- Own Profile-->
        <% include ../partials/workspace/sidebars/own_profile.ejs %>
        <!--./Own Profile-->
      </div>
      <!-- ./ content -->
    </div>
    <!-- ./ layout -->

    <!-- Socket io -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js"></script>
    <!-- Bundle -->
    <script src="./sl/vendor/bundle.js"></script>
    <script src="./sl/vendor/enjoyhint/enjoyhint.min.js"></script>
    <!-- App scripts -->
    <script src="./sl/dist/js/app.js"></script>
    <script src="./sl/dist/js/workspace.js"></script>
    <script src="./sl/dist/js/Contact.js"></script>
    <script src="./sl/dist/js/delete.js"></script>
    <!-- Examples -->
    <script src="./sl/dist/js/examples.js"></script>
  </body>
</html>
